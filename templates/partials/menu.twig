{% for item in menu %}
  <li class="{{item.classes | join(' ')}} {{ (item.has_child_class) ? 'dropdown' : null }}"	data-toggle>
    {% include 'menu-item.twig' %}
  </li>
{% endfor %}

{% if is_woocommerce_active == true %}
  {% include './woocommerce/menu-item-my-account.twig' %}
{% endif %}

<li class="divider"></li>

{% for item in menu_top %}
  <li class="hidden-expanded {{item.classes | join(' ')}} {{ (item.has_child_class) ? 'dropdown' : null }}"	data-toggle>
    {% include 'menu-item.twig' %}
  </li>
{% endfor %}
